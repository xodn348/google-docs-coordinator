{
  "manifest_version": 3,
  "name": "Google Docs Coordinator",
  "version": "1.0.0",
  "description": "AI-powered collaboration analysis for Google Docs.",
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://docs.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/document/d/*"],
      "js": ["content.js"]
    }
  ]
}
